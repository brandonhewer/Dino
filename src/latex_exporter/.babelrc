{
  "presets": [
    "airbnb", 
    [
      "@babel/preset-env",
      {
        "modules": "commonjs"
      }
    ], 
    "@babel/preset-react",
    [
      "@babel/preset-typescript",
      {
        "allExtensions": true,
        "isTSX": true,
        "jsxPragma": "h",
      }
    ]
  ],
  "plugins": [
    ["babel-plugin-root-import"],
    ["@babel/plugin-transform-react-jsx", {"pragma": "h"}]
  ]
}